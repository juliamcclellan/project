class Entry
  
  def initialize(time_entry, name, entry)
    @time_entry = time_entry
    @name = name
    @entry = entry
  end
  
  def add_collaborator(collaborator)
    @collaborator = collaborator
  end
  
  def entryString
    "#{@name} made an entry at #{@time_entry.toString}: #{@entry}"
  end

end

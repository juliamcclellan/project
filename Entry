class Entry
  
  initialize(time_entry, name, entry)
    @time_entry = time_entry
    @name = name
    @entry = entry
  end
  
  def add_collaborator(collaborator)
    @collaborator = collaborator
  end
  
  def entryString
    "#{name} made an entry at #{time_entry}: entry"
  end

#to end the class, use $end instead of just end
$end
